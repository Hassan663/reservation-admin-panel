  "scripts": {
    "start": "cross-env PORT=3000 react-scripts --openssl-legacy-provider start",
    "start:qa": "cross-env PORT=3000  REACT_APP_ENV=qa react-app-rewired start",
    "start:prod": "cross-env PORT=3000 REACT_APP_ENV=prod react-app-rewired start",
    "build": "react-scripts build",
    "build:dev": "cross-env REACT_APP_ENV=dev GENERATE_SOURCEMAP=true react-app-rewired --max_old_space_size=4096 build",
    "build:qa": "cross-env REACT_APP_ENV=qa GENERATE_SOURCEMAP=true react-app-rewired --max_old_space_size=4096 build",
    "build:prod": "cross-env REACT_APP_ENV=prod GENERATE_SOURCEMAP=false react-app-rewired build",
    "deploy": "npm run deploy:dev",
    "deploy:dev": "cross-env BUCKET=BUCKET_PATH DISTRIBUTION_ID=E2Y6YDB6QRO8UA npm run publish",
    "deploy:qa": "cross-env BUCKET=BUCKET_PATH DISTRIBUTION_ID=EA8655HCMIDNY npm run publish",
    "deploy:prod": "cross-env BUCKET=BUCKET_PATH DISTRIBUTION_ID=E2BLXLQG334BAL npm run publish",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublishOnly": "cross-env aws s3 rm $BUCKET --recursive",
    "publish": "cross-env-shell aws s3 sync build $BUCKET --exclude 'index.html' --acl public-read --cache-control 'max-age=860000'",
    "postpublish": "cross-env-shell aws s3 sync build $BUCKET --exclude '*' --include 'index.html' --acl public-read --cache-control 'max-age=0'",
    "lint": "eslint src/ -c .eslintrc.js --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
